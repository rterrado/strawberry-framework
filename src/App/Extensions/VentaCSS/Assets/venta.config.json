{
	"config":{
		"minify":true
	},
	"colors": {
		"primary": "#009688",
		"secondary": "#3f51b5",
		"warning": "#ffc107",
		"danger": "#f44336",
		"info": "#2196f3",
		"link": "#03a9f4",
		"dark": "#2a2a2a",
		"gray": "#b1b1b1",
		"light-gray": "#efefef",
		"white": "white",
		"black": "black"
	},
	"breakpoints": [{
		"max": 370,
		"min": 240
	}, {
		"max": 501,
		"min": 371
	}, {
		"max": 632,
		"min": 502
	}, {
		"max": 763,
		"min": 633
	}, {
		"max": 894,
		"min": 764
	}, {
		"max": 1025,
		"min": 895
	}, {
		"max": 1156,
		"min": 1026
	}, {
		"max": 1287,
		"min": 1157
	}, {
		"max": 1418,
		"min": 1288
	}, {
		"max": 1549,
		"min": 1419
	}, {
		"max": 1680,
		"min": 1550
	}, {
		"max": 1811,
		"min": 1681
	}],
	"width": {
		"rules": ["width"],
		"unit": "%",
		"max": "100",
		"min": "0",
		"slice": "24",
		"delimiter": "-"
	},
	"max-width": {
		"rules": ["max-width"],
		"unit": "px",
		"max": "2000",
		"min": "100",
		"slice": "24",
		"delimiter": "-"
	},
	"min-width": {
		"rules": ["min-width"],
		"unit": "px",
		"max": "2000",
		"min": "100",
		"slice": "24",
		"delimiter": "-"
	},
	"item-width": {
		"rules": ["width"],
		"unit": "px",
		"max": "420",
		"min": "120",
		"slice": "24",
		"delimiter": "-"
	},
	"small-width": {
		"rules": ["width"],
		"unit": "px",
		"max": "100",
		"min": "1",
		"slice": "24",
		"delimiter": "-"
	},
	"height": {
		"rules": ["height"],
		"unit": "%",
		"max": "100",
		"min": "10",
		"slice": "24",
		"delimiter": "-"
	},
	"device-height": {
		"rules": ["height"],
		"unit": "vh",
		"max": "100",
		"min": "10",
		"slice": "24",
		"delimiter": "-"
	},
	"min-height": {
		"rules": ["min-height"],
		"unit": "vh",
		"max": "100",
		"min": "10",
		"slice": "24",
		"delimiter": "-"
	},
	"margin": {
		"rules": null,
		"unit": "em",
		"max": "2",
		"min": "0.01",
		"slice": "24",
		"delimiter": "-"
	},
	"padding": {
		"rules": null,
		"unit": "em",
		"max": "2",
		"min": "0.01",
		"slice": "24",
		"delimiter": "-"
	},
	"text": {
		"rules": ["font-size"],
		"unit": "em",
		"max": "1.8",
		"min": "0.8",
		"slice": "24",
		"delimiter": "-"
	},
	"line-height": {
		"rules": ["line-height"],
		"unit": "em",
		"max": "1.8",
		"min": "0.8",
		"slice": "24",
		"delimiter": "-"
	},
	"letter-spacing": {
		"rules": ["letter-spacing"],
		"unit": "em",
		"max": "0.13",
		"min": "0.01",
		"slice": "24",
		"delimiter": "-"
	}
}
